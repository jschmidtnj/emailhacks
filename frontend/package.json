{
  "name": "mail-pear",
  "version": "1.0.0",
  "description": "progressive email generator",
  "author": "Joshua",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "email:build": "mjml emailtemplates/*.mjml -o static/emailtemplates/",
    "email:dev": "mjml -u emailtemplates/**/*.mjml",
    "build": "yarn email:build && nuxt-ts build",
    "build:spa": "nuxt build --spa",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "precommit": "yarn lint",
    "predeploy": "yarn precommit && yarn email:build && yarn generate",
    "serve": "node staticserver.js",
    "now-build": "yarn predeploy",
    "deploy:gcp": "gcloud app deploy",
    "deploy:heroku": "sh ./deployheroku.sh",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jschmidtnj/emailhacks"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@nuxt/typescript-runtime": "^0.3.3",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/google-analytics": "^2.2.2",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@types/vuelidate": "^0.7.9",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "cross-env": "^5.2.0",
    "epic-spinners": "^1.1.0",
    "express": "^4.16.4",
    "highlight.js": "^9.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "mjml": "^4.5.1",
    "nuxt": "^2.0.0",
    "nuxt-webfontloader": "^1.1.0",
    "particles.js": "^2.0.0",
    "prismjs": "^1.17.1",
    "tiptap": "^1.26.6",
    "tiptap-extensions": "^1.28.6",
    "vue-pdf": "^4.0.7",
    "vue-recaptcha-v3": "^1.8.0",
    "vue-scroll-reveal": "^1.0.11",
    "vue-select": "^3.4.0",
    "vue-toasted": "^1.1.27",
    "vue2-touch-events": "^2.1.0",
    "vuedraggable": "^2.23.2",
    "vuelidate": "^0.7.4",
    "vuex-module-decorators": "^0.11.0",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.2",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.4.1",
    "connect": "^3.7.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "pug": "2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.0",
    "serve-static": "^1.14.1",
    "vue-jest": "^4.0.0-0"
  }
}
